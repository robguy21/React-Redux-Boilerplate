{
  "launchers": {
    "Chrome": {
      "command": "babel-tape-runner ./src/assets/js/**/*.spec.js"
    },
    "PhantomJS": {
      "command": "babel-tape-runner ./src/assets/js/**/*.spec.js"
    }
  },
  "launch_in_ci": [
    "PhantomJS"
  ],
  "framework": "tap",
  "src_files": [
    "./src/assets/js/**/*.js"
  ],
  "phantomjs_debug_port": 9001,
  "bail_on_uncaught_error": false,
  "launch_in_dev": ["PhantomJS", "Chrome"]
}